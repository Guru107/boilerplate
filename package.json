{
	"name": "boilerplate",
	"version": "1.0.0",
	"license": "MIT",
	"scripts": {
		"start": "better-npm-run dev",
		"production": "better-npm-run build",
		"server": "better-npm-run server",
		"clean": "rimraf ./public",
		"dev:prod": "better-npm-run dev:prod"
	},
	"betterScripts": {
		"dev": {
			"command": "webpack-dev-server --config webpack.config.js",
			"env": {
				"NODE_ENV": "development",
				"PORT": "8080"
			}
		},
		"build": {
			"command": "webpack --config webpack.config.js",
			"env": {
				"NODE_ENV": "production",
				"TIER": "production",
				"PORT": "80"
			}
		}
	},
	"dependencies": {
		"howler": "^2.0.3",
		"material-ui": "^0.18.1",
		"prop-types": "15.5.8",
		"react": "15.5.4",
		"react-dom": "15.5.4",
		"react-router": "4.1.1",
		"react-router-dom": "4.1.1",
		"react-tap-event-plugin": "^2.0.1"
	},
	"devDependencies": {
		"autoprefixer": "6.7.7",
		"babel-core": "6.24.1",
		"babel-loader": "6.4.1",
		"babel-plugin-transform-object-rest-spread": "6.23.0",
		"babel-preset-es2015": "6.24.1",
		"babel-preset-react": "6.24.1",
		"babel-preset-stage-1": "6.24.1",
		"better-npm-run": "0.0.15",
		"compression-webpack-plugin": "0.4.0",
		"css-loader": "0.28.0",
		"extract-text-webpack-plugin": "2.1.0",
		"file-loader": "0.11.1",
		"html-webpack-plugin": "2.28.0",
		"less": "2.7.2",
		"less-loader": "4.0.3",
		"nodemon": "1.11.0",
		"postcss-loader": "1.3.3",
		"precss": "1.4.0",
		"react-hot-loader": "3.0.0-beta.6",
		"rimraf": "2.6.1",
		"style-loader": "0.16.1",
		"svg-inline-loader": "0.7.1",
		"svg-inline-react": "2.1.0",
		"url-loader": "0.5.8",
		"webpack": "2.4.1",
		"webpack-dev-server": "2.4.2"
	}
}
